{
  "name": "paramorph",
  "version": "1.5.0",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf lib #",
    "compile-ts": "tsc",
    "compile-coffee": "coffee -c -m -o lib src",
    "compile": "npm run compile-ts && npm run compile-coffee",
    "test": "mocha --use_strict lib/mocha.config.js $(find lib -name \"*.spec.js\")",
    "copy-package.json": "mkdir -p lib/ && cp package.json lib/",
    "build": "npm run compile && npm run test && npm run copy-package.json",
    "clean-build": "npm run clean && npm run build",
    "deploy": "npm run clean-build && npm publish lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/muroc/paramorph.git"
  },
  "keywords": [
    "static",
    "website",
    "blog",
    "generator",
    "typescript",
    "react",
    "jekkyl"
  ],
  "author": "Maciej Cha≈Çapuk <maciej@chalapuk.pl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/muroc/paramorph/issues"
  },
  "homepage": "https://github.com/muroc/paramorph#readme",
  "description": "",
  "dependencies": {
    "loader-utils": "^1.1.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.1.1",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@types/loader-utils": "^1.1.0",
    "@types/react": "^16.0.20",
    "@types/react-dom": "^16.0.2",
    "@types/react-hot-loader": "^3.0.5",
    "@types/react-router-dom": "^4.2.0",
    "@types/webpack": "^3.0.14",
    "@types/webpack-env": "^1.13.2",
    "coffeescript": "^1.12.7",
    "mocha": "^3.5.3",
    "should": "^13.1.3",
    "should-sinon": "0.0.6",
    "typescript": "^2.6.1"
  }
}
